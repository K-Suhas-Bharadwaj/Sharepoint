            );*/

            SecureString password = new SecureString();
            foreach (char character in "Suhas@1999".ToCharArray())
            {
                password.AppendChar(character);
            }

            ICredentials credentials = new SharePointOnlineCredentials("SUHASBHARADWAJK@SuhasBharadwajK.onmicrosoft.com", password);

            DownloadFileViaRestAPI(SharepointURL, Site, FileLocationInSharepoint, FileName, FileDownloadLocation, credentials);

