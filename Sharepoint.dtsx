        public static void DownloadFileViaRestAPI(string SharepointURL, string Site, string FileLocationInSharepoint, string FileName, string FileDownloadLocation, ICredentials credentials)
        {
            SharepointURL = SharepointURL.EndsWith("/") ? SharepointURL.Substring(0, SharepointURL.Length - 1) : SharepointURL;
            string SiteURL = SharepointURL + "/sites/" + Site;
            using (WebClient client = new WebClient())
            {
                //MessageBox.Show("Inside Function");
                client.Headers.Add("X-FORMS_BASED_AUTH_ACCEPTED", "f");
                client.Credentials = credentials;
                string url = SiteURL + "/_api/web/GetFileByServerRelativeUrl('" + FileLocationInSharepoint + "/" + FileName + "')/$value";
                //MessageBox.Show(url);
                Uri endpointUri = new Uri(url);
                //MessageBox.Show("Formed End point URI");
                byte[] data = client.DownloadData(endpointUri);
                //MessageBox.Show("Downloaded Data");
                FileStream outputStream = new FileStream(FileDownloadLocation + FileName, FileMode.OpenOrCreate | FileMode.Append, FileAccess.Write, FileShare.None);
                outputStream.Write(data, 0, data.Length);
                outputStream.Flush(true);
                outputStream.Close();
            }

        }

